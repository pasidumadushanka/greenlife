{
  "builds": [
    {
      "src": "**/*.php",
      "use": "vercel-php@0.7.2"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(assets/.*)",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}