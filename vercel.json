{
  "builds": [
    {
      "src": "**/*.php",
      "use": "vercel-php@0.7.2"
    },
    {
      "src": "**/*.{html,css,js,jpg,jpeg,png,gif,svg,ico,mp4,ttf,woff,woff2}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}