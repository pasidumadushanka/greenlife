{
  "functions": {
    "admin/**/*.php": { "runtime": "vercel-php@0.6.0" },
    "client/**/*.php": { "runtime": "vercel-php@0.6.0" },
    "therapist/**/*.php": { "runtime": "vercel-php@0.6.0" },
    "config/**/*.php": { "runtime": "vercel-php@0.6.0" },
    "includes/**/*.php": { "runtime": "vercel-php@0.6.0" },
    "*.php": { "runtime": "vercel-php@0.6.0" }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}